    "effector-react": ">=23.0.0",
    "lodash.topath": ">=4.5.2",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "lodash.topath": "^4.5.2"
  },
